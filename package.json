{
  "name": "astro-with-trpc",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "db:generate": "pnpm prisma generate",
    "db:migrate-dev": "pnpm prisma migrate dev --name init",
