    "db:generate": "drizzle-kit generate:sqlite --schema=./src/lib/db/schema --out=./src/lib/db/migrations",
    "db:migrate": "wrangler d1 migrations apply teknodaim --remote",
    "db:migrate:local": "wrangler d1 migrations apply teknodaim --local",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check:sqlite",
    "format:fix": "prettier --write \"./src/**/*.{ts,tsx,js,jsx,css,md,mdx,json,astro}\"",
