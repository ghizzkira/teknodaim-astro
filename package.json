    "@prisma/client": "^5.12.1",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "astro": "^4.6.1",
    "auth-astro": "^4.1.1",
    "cookie": "^0.6.0",
    "tailwindcss": "^3.4.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@tailwindcss/forms": "^0.5.7",
