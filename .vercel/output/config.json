{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth(?:\\/(.*?))?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/trpc\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/auth\\/sign-in\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/author\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/tag\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/([^/]+?)\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}